<Application x:Class="Mwman.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mwman="clr-namespace:Mwman"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
            <mwman:ViewModelLocator x:Key="ViewModelLocator"></mwman:ViewModelLocator>
             <ResourceDictionary.MergedDictionaries>
                 <ResourceDictionary Source="ItemsStyles.xaml"></ResourceDictionary>
             </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
